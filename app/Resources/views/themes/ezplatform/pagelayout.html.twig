<!DOCTYPE html>
<html class="no-js" lang="en">
    <head>
        <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
        <meta http-equiv="Content-Language" content="en">
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">

        {% block head_styles %}
            <link rel="shortcut icon" href="{{ asset('favicon.ico') }}" type="image/x-icon">
            <link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/font-awesome/4.6.3/css/font-awesome.min.css">
            <link rel="stylesheet" href="//fonts.googleapis.com/css?family=Open+Sans+Condensed:300,700|Open+Sans:400,600">
            {{ encore_entry_link_tags('ezp-com') }}
        {% endblock %}

        {% block head_scripts %}
        {% endblock %}

        {% block head_tracking %}
            {% include '@ezdesign/layout/tracking_head.html.twig' only %}
        {% endblock %}

        {% block canonical %}
        {% endblock %}

        {% block seo_metas %}
            {% include "NovaeZSEOBundle::seometas_head.html.twig" %}
        {% endblock %}

<style>


</style>

    </head>
    <body data-ga-category="{% block gaCategory %}{% endblock %}" class="object-{{ content.id|default('none')}} location-{{ location.id|default('none')}}">
        {% block main %}
            <main>
        {% endblock %}

            {% block header %}
                {% include '@ezdesign/layout/header.html.twig' only %}
            {% endblock %}

            {% block content %}
            {% endblock %}

            {% block footer %}
                {{ render(controller('AppBundle:Layout:showPart', {
                      'template': '@ezdesign/layout/footer.html.twig'
                })) }}
            {% endblock %}
        </main>

        {% block javascripts %}
            <script src="{{ asset('bundles/ezplatformadminuiassets/vendors/jquery/dist/jquery.min.js') }}"></script>
            <script src="{{ asset('bundles/ezplatformadminuiassets/vendors/popper.js/dist/umd/popper.min.js') }}"></script>
            <script src="{{ asset('bundles/ezplatformadminuiassets/vendors/bootstrap/dist/js/bootstrap.min.js') }}"></script>
            <script src="//cdnjs.cloudflare.com/ajax/libs/lightbox2/2.10.0/js/lightbox.min.js"></script>

            {{ encore_entry_script_tags('ezp-com') }}
        {% endblock %}
    </body>
</html>
